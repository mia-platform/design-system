{
  "palette": {
    "action": {
      "alternate": {
        "danger": {
          "active": {
            "$type": "color",
            "$value": "{palette.action.alternate.danger.disabled}"
          },
          "disabled": {
            "$type": "color",
            "$value": "{colors.Neutral.100A}"
          },
          "focus": {
            "$type": "color",
            "$value": "#0000000a"
          },
          "hover": {
            "$type": "color",
            "$value": "{palette.action.alternate.danger.active}"
          },
          "selected": {
            "$type": "color",
            "$value": "{colors.Red.300A}"
          }
        }
      }
    }
  },
  "shape": {
    "border": {
      "radius": {
        "sharp": {
          "$type": "dimension",
          "$value": "{sizing.none}"
        },
        "anotherSharp": {
          "$type": "dimension",
          "$value": "{shape.border.radius.sharp}"
        }
      }
    }
  }
}
